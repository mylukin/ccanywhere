{
  "repo": {
    "kind": "github",
    "url": "https://github.com/mylukin/my-project",
    "branch": "main"
  },
  "deployment": {
    "webhook": "https://deploy.example.com/api/webhook/deploy",
    "statusUrl": "https://deploy.example.com/api/status",
    "maxWait": 300,
    "pollInterval": 5
  },
  "notifications": {
    "channels": ["telegram"],
    "telegram": {
      "botToken": "YOUR_BOT_TOKEN",
      "chatId": "YOUR_CHAT_ID"
    },
    "email": {
      "to": "admin@example.com",
      "from": "noreply@example.com"
    },
    "dingtalk": {
      "webhook": "https://oapi.dingtalk.com/robot/send?access_token=YOUR_TOKEN",
      "secret": "YOUR_SECRET"
    },
    "wecom": {
      "webhook": "https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=YOUR_KEY"
    }
  },
  "build": {
    "base": "origin/main",
    "lockTimeout": 300,
    "cleanupDays": 7
  },
  "test": {
    "enabled": true,
    "configFile": "./playwright.config.ts"
  },
  "artifacts": {
    "baseUrl": "https://artifacts.example.com",
    "retentionDays": 7,
    "maxSize": "100MB",
    "storage": {
      "provider": "s3",
      "s3": {
        "accessKeyId": "YOUR_AWS_ACCESS_KEY_ID",
        "secretAccessKey": "YOUR_AWS_SECRET_ACCESS_KEY",
        "region": "us-east-1",
        "bucket": "my-artifacts-bucket"
      },
      "r2": {
        "accountId": "YOUR_CLOUDFLARE_ACCOUNT_ID",
        "accessKeyId": "YOUR_R2_ACCESS_KEY_ID", 
        "secretAccessKey": "YOUR_R2_SECRET_ACCESS_KEY",
        "bucket": "my-r2-bucket"
      },
      "oss": {
        "accessKeyId": "YOUR_ALIYUN_ACCESS_KEY_ID",
        "accessKeySecret": "YOUR_ALIYUN_ACCESS_KEY_SECRET",
        "region": "oss-cn-hangzhou",
        "bucket": "my-oss-bucket"
      }
    }
  }
}