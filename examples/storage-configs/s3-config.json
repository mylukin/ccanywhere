{
  "repo": {
    "kind": "github",
    "url": "https://github.com/mylukin/my-project",
    "branch": "main"
  },
  "artifacts": {
    "baseUrl": "https://cdn.example.com/artifacts",
    "retentionDays": 7,
    "maxSize": "100MB",
    "storage": {
      "provider": "s3",
      "s3": {
        "accessKeyId": "AKIA...",
        "secretAccessKey": "your-secret-key",
        "region": "us-east-1",
        "bucket": "my-artifacts-bucket"
      }
    }
  },
  "deployment": {
    "webhook": "https://deploy.example.com/api/webhook/deploy"
  },
  "notifications": {
    "channels": ["telegram"],
    "telegram": {
      "botToken": "YOUR_BOT_TOKEN",
      "chatId": "YOUR_CHAT_ID"
    }
  }
}